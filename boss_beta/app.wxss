/**app.wxss**/
:before,:after {
font-family:'icomoon' !important;
speak:none;
font-style:normal;
font-weight:normal;
font-variant:normal;
text-transform:none;
line-height:1;
-webkit-font-smoothing: antialiased;
-moz-osx-font-smoothing: grayscale;
} 
page {font-size:4.2vmin;background:#f0eff5;display:block;min-height:100%;border-bottom:1rpx solid rgba(0,0,0,0);}

.gap {display:block;border-top:5vmin solid #f0eff5;}

.heading,
.heading01 {display:block;padding:2.5vmin;font-size:4.5vmin;font-weight:bold;vertical-align:middle;}

.heading01 {border-bottom:.5pt solid #e1e1e1;}

.heading02 {color:#222;padding:2.5vmin 5vmin;font-size:4.5vmin;}
.heading02 .tip {color:#999;font-size:4vmin;margin-left:2vmin;}
.heading02.warn {background:#fcedeb;color:#f43530;text-align:left;}
.heading02.warning {color:#fcedeb;background:#f43530;text-align:left;}
.heading02 .success {background:#c8faaf;color:#0cc725;text-align:left;}
.heading02.icon_warn:before {content:"";width:6vmin;height:6vmin;display:inline-block;vertical-align:middle;background:url(http://jobnotes.cn/bmimg/warn_red.svg) no-repeat 50% 50% / 6vmin;margin-right:3vmin;}

.heading03 {color:#06123c;padding:4vmin 5vmin;font-size:5vmin;line-height:1;font-weight:bold;}
.heading04 {color:#06123c;margin:0 5vmin;padding:2vmin 0;font-size:4vmin;line-height:1;border-bottom:1rpx solid #ddd;font-weight:bold;}
.heading04:before {content:"";border-left:1vmin solid #06123c;margin-right:3vmin;display:inline-block;height:4vmin;vertical-align:bottom;}

.heading_gray {color:#999;padding:2.5vmin 5vmin;font-size:4.5vmin;}

.icon_account {padding-left:13vmin;position:relative;}
.icon_account:before {content:"";position:absolute;left:5vmin;top:3vmin;background:url(http://jobnotes.cn/bmimg/account.svg) no-repeat 50% 50% / 5.3vmin;width:5.3vmin;height:5.3vmin;}
.icon_other {padding-left:13vmin;position:relative;}
.icon_other:before {content:"";position:absolute;left:5vmin;top:3vmin;background:url(http://jobnotes.cn/bmimg/other.svg) no-repeat 50% 50% / 5.3vmin;width:5.3vmin;height:5.3vmin;}
.icon_bill {padding-left:13vmin;position:relative;}
.icon_bill:before {content:"";position:absolute;left:5vmin;top:3vmin;background:url(http://jobnotes.cn/bmimg/bill.svg) no-repeat 50% 50% / 5.3vmin;width:5.3vmin;height:5.3vmin;}
.icon_yen {padding-left:8vmin;position:relative;}
.icon_yen:before {content:"";position:absolute;left:0;top:1vmin;background:url(http://jobnotes.cn/bmimg/yen.svg) no-repeat 50% 50% / 5.3vmin;width:5.3vmin;height:5.3vmin;}

.topbox {background:#06123c;color:#fff;padding-bottom:5vmin;}
.topbox text {display:block;font-size:7vmin;margin:0 5vmin;}
