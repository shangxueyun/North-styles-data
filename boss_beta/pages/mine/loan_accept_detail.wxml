<!--//受理中贷款详情//-->
<view class='whiteBox'>
  <view class='list01'>
    <view class='item'>
      <view class='item_in2 gray'>状态</view>
      <view class='item_in2'>受理中</view>
    </view>
    <view class='item'>
      <view class='item_in2 gray'>申请贷款金额</view>
      <view class='item_in2'>{{data.loanAmt}}元</view>
    </view>
    <view class='item'>
      <view class='item_in2 gray'>贷款利率</view>
      <view class='item_in2'>{{data.loanRate}}%</view>
    </view>
    <view class='item'>
      <view class='item_in2 gray'>应还利息</view>
      <view class='item_in2'>{{data.interest}}元</view>
    </view>
    <view class='item'>
      <view class='item_in2 gray'>贷款期限</view>
      <view class='item_in2'>{{data.loanTerm + data.termUnit}}</view>
    </view>
    <view class='item'>
      <view class='item_in2 gray'>发起申请日</view>
      <view class='item_in2'>{{data.gmtCreate}}</view>
    </view>
    <view class='item'>
      <view class='item_in2 gray'>应还总额</view>
      <view class='item_in2'>{{data.Total}}元</view>
    </view>
