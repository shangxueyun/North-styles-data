page {background:#fff;}
.img-show-realize_face{
  width: 100%
}
.canvas{
  width:250px;
  height:310px;
  position: absolute;
  border-radius:1.5vmin;
  z-index: 1;
}
.positionA{
  z-index: 111;
  width:250px;
  height:310px;
}
.poper{
  position: fixed;
  z-index:1111111;
}
.poper .popbox {
  width:80vmin;
  left:10vmin;
  bottom:calc((100vh - 60vmin) / 2);
  position: fixed;
  z-index:1111111;
}
.poper .icon_ok {
  margin-bottom:2vmin;
  padding-top:2vmin;

}
.poper .submitbox {
  margin-top:0vmin;
}
.poper .center {
  padding-bottom:1vmin;
}
.Hides{
  display: none;
}
  