text.username {text-align:center;font-size:4.2vmin;}

.item_key .big {font-size:8vmin;line-height:1;}
.item_val {text-align:right;position:absolute;right:0;top:3vmin;}
.list input {font-size:5vmin;line-height:1;}
.reddot {display:block;width:4vmin;height:4vmin;margin-top:1.5vmin;background:url(http://jobnotes.cn/bmimg/icon_reddot.svg) no-repeat 50% 50% / 3vmin}

.list {margin:4vmin 5vmin;}
.list .item {display:block;line-height:1.5;position:relative;}
.list .item text {line-height:2;}
