.list01 .item_abs {position:absolute;z-index:10;right:0;top:0;text-align:right;}
.list01 .item_val.arrow {width:auto;padding-right:2.5vmin;}
.list01 .item_val text:first-child {font-size:4.2vmin}
.list01 .item_val text:last-child {color:#888;font-size:3.5vmin}
.list01 .blue {color:#33b9ff;}
.list01 .darkblue {color:#16286d;}
.list01 .green {color:#11ab0f;}
.list01 .warn {color:#f43530;}
.list01 .orange {color:#fa9611;}
.list01 .red {color:#ff6363;}
.list01 .gray {color:#888;}
.list01 .checkcode_img {width:25vmin;margin-top:1vmin;}
.list01 .button_white {margin-top:1vmin}

.list02 {margin:2.5vmin}
.list02 .item {position:relative;width:50%;display:inline-block;vertical-align:top;}
.list02 .innerbox {display:block;border:1px solid rgba(0,0,0,.0);margin:2vmin;background:#efeff4;border-radius:1.5vmin;min-height:20vmin;text-align:center;color:#333;padding:4vmin 0;}
.list02 .important:before {content:"*";color:#f43530;font-weight:bold;}
.list02 .item text {display:block;font-size:3.8vmin;}
.list02 .item .icon_plus {margin-bottom:1vmin;}
.list02 .item .icon_plus:before {content:"";background:url(http://jobnotes.cn/bmimg/plus_gray.svg) no-repeat 50% 50% / 6vmin;display:inline-block;width:6vmin;height:6vmin;}
.list02 .item image {width:60%;margin-bottom:1vmin;}
.list02 .item_4 {display:inline-block;vertical-align:bottom;width:25%;text-align:center;}
.list02 .item_4 text {display:block;}
.list02 .item_4 image {width:90%;margin-bottom:1vmin;}
.list02 .item_4 .innerbox {background:#fff;min-height:auto;}
.list02 .icon_camera {position:absolute;left:calc((100% - 12vmin) / 2);top:25%;}
.list02 .icon_camera:before {background-size:12vmin;width:12vmin;height:12vmin;}

.item_hover {background:#e6e6eb;color:#000}

.tailbox {margin:5vmin 0;text-align:center;color:#999;font-size:4.2vmin;}

.boxer {margin:5vmin}

.btnbox {margin:5vmin;}
.button_white {padding:2vmin 3vmin;border:1rpx solid #000;border-radius:2vmin;}
.button_white_hover {background:rgba(0,0,0,.1);}
.button_gray {border:0;background:#9196aa;color:#fff;}
.button_gray_hover {border:0;background:#858a9b;color:#fff;}
.button_dark {border:0;background:#06123b;color:#fff;}
.button_dark_hover {border:0;background:#2e6bbe;color:#fff;}
.check_code_btn {display:block;font-size:4.2vmin;height:7vmin;line-height:7vmin;color:#2e6bbe}
.check_code_txt {display:block;font-size:4.2vmin;height:7vmin;line-height:7vmin;}

.poper {display:block;width:100vw;height:100vh;background:rgba(0,0,0,.6);position:fixed;z-index:100;left:0;top:0;}
.poper .popbox {position:absolute;width:80vmin;left:10vmin;bottom:calc((100vh - 80vmin) / 2);border-radius:1vmin;background:#fff;}
.poper .headingbox {text-align:center;}
.poper .headingbox .icon_ok {margin-bottom:6vmin;}
.poper .headingbox .icon_ok:before {content:"";width:6vmin;height:6vmin;margin-top:0;margin-right:3vmin;background:url(http://jobnotes.cn/bmimg/ok.svg) no-repeat 50% 50% / 6vmin;}
.poper .heading {display:inline-block;border:0;line-height:1;height:5vmin;padding:0;font-size:5vmin;margin:8vmin 0 4vmin 0;}
.poper .paragraph {display:block;padding:0 8vmin;color:#999;line-height:1.8;}
.poper .center {text-align:center;}
.poper .submitbox {border-top:1rpx solid #ddd;margin-top:5vmin;border-radius:0 0 1vmin 1vmin;}
.poper .submitbox .btn {background:none;font-size:5vmin;border-radius:0;}
.poper .submitbox .btn_half {display:inline-block;width:50%;background:none;font-size:5vmin;border-radius:0;}
.poper .submitbox .btn:after,
.poper .submitbox .btn_half:after {background:none;border:0;margin:0;}
.poper .submitbox .btn_hover,
.poper .submitbox .btn_half_hover {background:rgba(0,0,0,.05);}
.poper .icon_tip_red {margin-bottom:6vmin;}
.poper .icon_tip_red:before {width:10vmin;height:10vmin;margin-right:0;background:url(http://jobnotes.cn/bmimg/warn_red.svg) no-repeat 50% 50% / 10vmin;}
.poper .icon_ok {margin-bottom:6vmin;}
.poper .icon_ok:before {content:"";width:10vmin;height:10vmin;margin-top:0;margin-right:0;background:url(http://jobnotes.cn/bmimg/ok.svg) no-repeat 50% 50% / 10vmin;}


.step_tab {background:#06123c;color:rgba(255,255,255,.5);display:block;width:100%;padding:5vmin 0;}
.step_tab .step_tab_item {display:inline-block;width:33.33%;text-align:center;}
.step_tab .step_tab_item text {display:block;padding-top:2vmin;line-height:1;font-size:3.5vmin;}
.step_tab .step_tab_1,
.step_tab .step_tab_2,
.step_tab .step_tab_3 {position:relative;}
.step_tab .step_tab_1:before,
.step_tab .step_tab_2:before,
.step_tab .step_tab_3:before {content:"";display:inline-block;width:6vmin;height:6vmin;opacity:.5;}
.step_tab .step_tab_1:before {background:url(http://jobnotes.cn/bmimg/step_1.svg) no-repeat 50% 50% / 6vmin;}
.step_tab .step_tab_2:before {background:url(http://jobnotes.cn/bmimg/step_2.svg) no-repeat 50% 50% / 6vmin;}
.step_tab .step_tab_3:before {background:url(http://jobnotes.cn/bmimg/step_3.svg) no-repeat 50% 50% / 6vmin;}
.step_tab .step_tab_1:after,
.step_tab .step_tab_2:after {content:"";border-top:1px solid rgba(255,255,255,.5);display:block;width:70%;position:absolute;right:-35%;top:2.8vmin;}
.step_tab .on,
.step_tab .done {color:#fff;}
.step_tab .on .step_tab_1:before,
.step_tab .on .step_tab_2:before,
.step_tab .on .step_tab_3:before {opacity:1;}
.step_tab .done .step_tab_1:before,
.step_tab .done .step_tab_2:before,
.step_tab .done .step_tab_3:before {background:url(http://jobnotes.cn/bmimg/ok_white.svg) no-repeat 50% 50% / 6vmin;opacity:1;}

.step_tab_white {background:#fff;color:rgba(6,17,59,.5);display:block;width:100%;padding:5vmin 0;}
.step_tab_white .step_tab_item {display:inline-block;width:33.33%;text-align:center;}
.step_tab_white .step_tab_item text {display:block;padding-top:2vmin;line-height:1;font-size:3.5vmin;}
.step_tab_white .step_tab_1,
.step_tab_white .step_tab_2,
.step_tab_white .step_tab_3 {position:relative;}
.step_tab_white .step_tab_1:before,
.step_tab_white .step_tab_2:before,
.step_tab_white .step_tab_3:before {content:"";display:inline-block;width:6vmin;height:6vmin;opacity:.5;}
.step_tab_white .step_tab_1:before {background:url(http://jobnotes.cn/bmimg/step_dark_1.svg) no-repeat 50% 50% / 6vmin;}
.step_tab_white .step_tab_2:before {background:url(http://jobnotes.cn/bmimg/step_dark_2.svg) no-repeat 50% 50% / 6vmin;}
.step_tab_white .step_tab_3:before {background:url(http://jobnotes.cn/bmimg/step_dark_3.svg) no-repeat 50% 50% / 6vmin;}
.step_tab_white .step_tab_1:after,
.step_tab_white .step_tab_2:after {content:"";border-top:1px dotted #06113b;display:block;width:70%;position:absolute;right:-35%;top:2.8vmin;}
.step_tab_white .on,
.step_tab_white .done {color:#06113b;}
.step_tab_white .on .step_tab_1:before,
.step_tab_white .on .step_tab_2:before,
.step_tab_white .on .step_tab_3:before {opacity:1;}
.step_tab_white .done .step_tab_1:before,
.step_tab_white .done .step_tab_2:before,
.step_tab_white .done .step_tab_3:before {background:url(http://jobnotes.cn/bmimg/ok_dark.svg) no-repeat 50% 50% / 6vmin;opacity:1;}

