.paddingBox {padding:4vmin 5vmin;}

.paragraphBox {padding:4vmin 5vmin;}
.paragraphBox .text {display:block;line-height:1.8;text-align:justify;}

.iconbox {padding-top:5vmin;text-align:center;}
.iconbox [class^="icon_"],
.iconbox [class*=" icon_"] {margin:0;padding:0;}

.txtBox {display:block;margin:5vmin;margin-top:0;}

.stateBox {text-align:center;padding:20vmin 10vmin;}
