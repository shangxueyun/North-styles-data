.icon_money:before {background:url(http://jobnotes.cn/bmimg/money.svg) no-repeat 50% 50% / 6vmin;}
.icon_wallet:before {background:url(http://jobnotes.cn/bmimg/wallet.svg) no-repeat 50% 50% / 6vmin;}
.icon_settings:before {background:url(http://jobnotes.cn/bmimg/settings.svg) no-repeat 50% 50% / 6vmin;}
.icon_file_time:before {background:url(http://jobnotes.cn/bmimg/file_time.svg) no-repeat 50% 50% / 6vmin;}
.icon_card_ok:before {background:url(http://jobnotes.cn/bmimg/card_ok.svg) no-repeat 50% 50% / 6vmin;}
.icon_lock:before {background:url(http://jobnotes.cn/bmimg/lock.svg) no-repeat 50% 50% / 6vmin;}
.icon_phone:before {background:url(http://jobnotes.cn/bmimg/phone.svg) no-repeat 50% 50% / 6vmin;}
.icon_mail:before {background:url(http://jobnotes.cn/bmimg/mail.svg) no-repeat 50% 50% / 6vmin;}
.icon_firm:before {background:url(http://jobnotes.cn/bmimg/firm.svg) no-repeat 50% 50% / 6vmin;}
.icon_contract:before {background:url(http://jobnotes.cn/bmimg/contract.svg) no-repeat 50% 50% / 6vmin;}

.arrow {width:10vmin;height:6vmin;background:url(http://jobnotes.cn/bmimg/icon_arrow01.svg) no-repeat 100% 50% / 4vmin;}

.icon_ok:before {content:"";width:6vmin;height:6vmin;display:inline-block;vertical-align:middle;margin-top:-1vmin;margin-right:2vmin;background:url(http://jobnotes.cn/bmimg/ok.svg) no-repeat 50% 50% / 6vmin;}

.icon_tip:before {content:"";width:6vmin;height:6vmin;display:inline-block;vertical-align:middle;margin-top:-1vmin;margin-right:2vmin;background:url(http://jobnotes.cn/bmimg/warn.svg) no-repeat 50% 50% / 6vmin;}

.icon_tip_red:before {content:"";width:6vmin;height:6vmin;display:inline-block;vertical-align:middle;margin-top:-1vmin;margin-right:2vmin;background:url(http://jobnotes.cn/bmimg/warn_red.svg) no-repeat 50% 50% / 6vmin;}

.icon_camera:before {content:"";width:6vmin;height:6vmin;display:inline-block;vertical-align:middle;margin-top:-1vmin;margin-right:2vmin;background:url(http://jobnotes.cn/bmimg/camera.png) no-repeat 50% 50% / 6vmin;}

.icon_company:before {content:"";width:6vmin;height:6vmin;display:inline-block;vertical-align:middle;margin-top:-1vmin;margin-right:2vmin;background:url(http://jobnotes.cn/bmimg/company.png) no-repeat 50% 50% / 6vmin;}

.icon_contract:before {content:"";width:6vmin;height:6vmin;display:inline-block;vertical-align:middle;margin-top:-1vmin;margin-right:2vmin;background:url(http://jobnotes.cn/bmimg/contract.png) no-repeat 50% 50% / 6vmin;}

.icon_customer_service {}
.icon_customer_service:before {content:"";display:inline-block;width:5vmin;height:5vmin;vertical-align:middle;background:url(http://jobnotes.cn/bmimg/customer_service.svg) no-repeat 50% 50% / 5vmin;margin-right:2vmin;}

.icon_time,
.icon_warn,
.stateBox .icon_ok {margin-bottom:5vmin;text-align:center;}

.icon_time:before {content:"";width:20vmin;height:20vmin;display:inline-block;vertical-align:middle;background:url(http://jobnotes.cn/bmimg/time.svg) no-repeat 50% 50% / 20vmin;}

.icon_warn:before {content:"";width:20vmin;height:20vmin;display:inline-block;vertical-align:middle;background:url(http://jobnotes.cn/bmimg/warn_red.svg) no-repeat 50% 50% / 20vmin;}

.stateBox .icon_ok:before {content:"";width:20vmin;height:20vmin;display:inline-block;vertical-align:middle;margin-top:0;margin-right:0;background:url(http://jobnotes.cn/bmimg/ok.svg) no-repeat 50% 50% / 20vmin;}

.tabbox {color:#06113b;}
.tabbox .tabItem_in2 {display:inline-block;width:50%;text-align:center;}
.tabbox .tabItem_in3 {display:inline-block;width:33.33%;text-align:center;}
.tabbox .tabItem_in4 {display:inline-block;width:25%;text-align:center;}
.tabbox .column_line {border-right:1rpx solid #ddd;}
.tabbox .tabItem_in2 view {text-align:center;}
.tabbox .tabItem_in2 .tab_menu {display:inline-block;font-size:4.2vmin;padding:3vmin 0;}
.tabbox .tab_title {font-size:3vmin;text-align:center;}
.tabbox .title {font-size:5vmin;text-align:center;}
.tabbox .gray {color:#999;}
.tabbox .small {font-size:3.5vmin;}
.tabbox .tabTitleTxt {display:inline-block;}
.tabbox .tipbox {display:inline-block;position:relative;margin-left:2vmin;}
.tabbox .tipbox .tip {position:absolute;left:0;top:-7vmin;color:#ff625f;min-width:15vmin;text-align:left;padding:3vmin 0;}
.tabbox .tipbox .tip:before {content:"";display:inline-block;width:3.5vmin;height:3.5vmin;background:url(http://jobnotes.cn/bmimg/warn_red.svg) no-repeat 50% 50% / 3.5vmin;margin-right:1vmin;vertical-align: middle;margin-top:-1.1vmin;}
.tabbox .on .tab_menu {border-bottom:6rpx solid #121d44;}
.tab_bg_dark {background:#121d44;padding:3vmin 0;color:rgba(255,255,255,.5);}
.tab_bg_white {background:#fff;padding:3vmin 0;color:#121d44;}

.text {display:block;line-height:1.5;}
.gray {color:#999;}
.link {color:#2e6bbe;}
.link_hover {color:#000;}
.align_r {display:block;text-align:right;}
.align_c {display:block;text-align:center;}

.avatarbox {padding:4.2vmin;}
.avatar {display:block;margin:0 auto;width:24vmin;height:24vmin;border-radius:100vmin;overflow:hidden;}
