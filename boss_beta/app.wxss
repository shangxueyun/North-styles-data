.list01 {}
.list01 .item {position:relative;padding:3vmin 0;margin:0 4.2vmin;border-bottom:1rpx solid #e1e1e1;}
.list01 .item:last-child {border:0;}
.list01 .item text {display:block;line-height:1.2;}
.list01 .item .txt {color:#999;}
.list01 .item_in2,
.list01 .item_in3,
.list01 .item_in4 {display:inline-block;vertical-align:top;color:#343434;}
.list01 .item_in2 {width:50%;}
.list01 .item_in3 {width:33.33%;}
.list01 .item_in4 {width:25%;}
.list01 .item_in2:last-child,
